<!DOCTYPE html>
<html>
<head>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h2>Works Pending</h2>
  <table>
    <thead>
      <tr>
        <th>Child-ID</th>
        <th>Process Name</th>
      </tr>
    </thead>
    <tbody>
      {{#each child}}
      <tr>
        <td>{{this.ChildId}}</td>
        <td>{{this.processName}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</body>
</html>